    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "engines": {
    "node": ">=6.9.0",
    "npm": ">= 3"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "/node_modules/react",
      "/node_modules/react-dom",
      "/node_modules/react-addons-test-utils"
    ],
    "collectCoverage": true,
    "coverageFormats": [
      "json",
      "html"
    ]
  }
}
