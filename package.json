{
  "name": "material",
  "private": true,
  "version": "1.3.1",
  "main": "",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && NODE_ENV=production webpack --progress --bail --env dist -p",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "lint-sass": "sass-lint -c .sass-lint.yml 'src/styles/**/*.scss' -v -q",
    "posttest": "npm run lint",
    "reinstall": "rimraf node_modules && npm install",
    "serve:dev": "webpack-dev-server --open --env dev",
    "serve:dist": "webpack-dev-server NODE_ENV=production --open --env dist -p --progress",
    "start": "npm run serve:dev",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "classnames": "~2.2.5",
    "core-js": "~2.4.0",
    "echarts": "^3.6.1",
    "element-resize-event": "^2.0.9",
    "font-awesome": "~4.7.0",
    "history": "^4.6.1",
    "html5-desktop-notifications": "^1.0.0",
    "immutable": "^3.8.1",
    "jest": "^20.0.4",
    "jquery": "^3.2.1",
    "jquery-slimscroll": "~1.3.8",
    "material-ui": "^0.18.1",
    "memorystorage": "^0.11.0",
    "prop-types": "^15.5.10",
    "rc-queue-anim": "^1.0.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-dropzone": "^4.1.3",
    "react-facebook-login-component": "^0.9.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-html5video": "^2.4.0",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.4",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.6.1",
    "redux": "~3.6.0",
    "redux-actions": "^2.2.1",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.15.6",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015-native-modules": "^6.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "~4.0.0-alpha.6",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "enzyme-matchers": "^3.8.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "expect": "^1.20.2",
